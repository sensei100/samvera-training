<% content_for(:gscholar_meta) do %>
  <% unless @presenter.try(:doi).blank? %>
    <meta name="citation_doi" content="<%= @presenter.doi %>"/>
  <% end %>
  <% unless @presenter.try(:isbns).blank? %>
    <% @presenter.isbns.each do |isbn| %>
      <meta name="citation_isbn" content="<%= isbn %>"/>
    <% end %>
  <% end %>
<% end %>